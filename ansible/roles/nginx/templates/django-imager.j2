server {
    listen 80;
    server_name {{ nginx_server_name }};

    location = /favicon.ico { access_log off; log_not_found off; }
    location /static/ {
        root /home/ubuntu/django-imager/imagersite/imagersite/;
    }

    location / {
        include proxy_params;
        proxy_pass http://localhost:8000;
    }
}
